{
  "name": "functions",
  "type": "module",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy_functions": "firebase deploy --only functions --project=dev",
    "deploy_functions:prod": "firebase deploy --only functions --project=prod",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^6.0.2",
    "@googleapis/sheets": "^3.0.2",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.22.0",
    "firebase-tools": "^11.8.0",
    "google-auth-library": "^8.3.0",
    "nodemailer": "^6.7.8",
    "undici": "^5.11.0"
  },
  "private": true
}
