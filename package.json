{
  "name": "mailcheck",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "cp:404": "cp build/error/index.html build/404.html",
    "rm:404": "rimraf - build/404",
    "sitemap": "node --experimental-json-modules ./generate-sitemap.js",
    "postbuild": "npm run sitemap && npm run cp:404 && npm run rm:404",
    "deploy_hosting": "firebase deploy --only hosting --project=dev",
    "deploy_functions": "firebase deploy --only functions --project=dev",
    "deploy_hosting:prod": "firebase deploy --only hosting --project=prod",
    "deploy:prod": "firebase deploy --project=prod",
    "deploy_functions:prod": "firebase deploy --only functions --project=prod"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.4",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.5",
    "cssnano": "^5.0.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "fast-glob": "^3.2.6",
    "mdsvex": "^0.9.0",
    "postcss": "^8.2.13",
    "postcss-load-config": "^3.0.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-slug": "^4.0.1",
    "remark-abbr": "^1.4.1",
    "remark-github": "^10.0.1",
    "sass": "^1.34.1",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "xmlbuilder2": "^2.4.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1",
    "just-camel-case": "^4.0.2",
    "schema-dts": "^0.8.3",
    "svelte-inview": "^2.0.1",
    "unist-util-visit": "^2.0.3"
  },
  "url": "http://www.mailcheck.co"
}
